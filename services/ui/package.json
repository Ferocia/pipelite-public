{
  "name": "ui",
  "version": "1.0.0",
  "description": "The UI for the most over-engineered build light software on the planet",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf _build/${NODE_ENV:-development} && rm -rf static/*",
    "precompile": "npm run clean",
    "compile": "npm run compile_client && npm run compile_server",
    "compile_client": "NODE_ENV=${NODE_ENV:-development} webpack -c --progress --bail --config config/webpack/client",
    "compile_server": "NODE_ENV=${NODE_ENV:-development} webpack -c --progress --bail --config config/webpack/server",
    "devserver": "babel-node src/devserver",
    "lint": "eslint src --ext .js,.jsx",
    "start": "node _build/${NODE_ENV:-production}/server",
    "test": "echo \"Error: no test specified\" && exit 0",
    "webpack_dev_server": "NODE_ENV=development webpack-dev-server --host=0.0.0.0 --port=5001 --colors --config ./config/webpack/client"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "classnames": "2.2.3",
    "dotenv": "2.0.0",
    "history": "2.0.1",
    "humps": "1.0.0",
    "immutable": "3.7.6",
    "isomorphic-fetch": "2.2.0",
    "koa": "2.0.0-alpha.3",
    "koa-bodyparser": "3.0.0",
    "koa-logger": "2.0.0",
    "koa-route": "3.0.0",
    "koa-router": "5.4.0",
    "lodash": "4.8.2",
    "moment": "2.11.1",
    "normalizr": "2.0.1",
    "raven": "0.10.0",
    "raven-js": "2.3.0",
    "react": "0.14.8",
    "react-dom": "0.14.8",
    "react-redux": "4.0.0",
    "react-router": "2.0.1",
    "redux": "3.3.1",
    "redux-logger": "2.6.1",
    "redux-thunk": "2.0.1",
    "reselect": "2.2.1",
    "socket.io": "1.4.5",
    "superagent": "1.8.3"
  },
  "devDependencies": {
    "assets-webpack-plugin": "3.4.0",
    "babel": "6.5.2",
    "babel-cli": "6.6.5",
    "babel-core": "6.7.4",
